public class pepyescheck {
  public static void main(String[] args) {
    int dieRoll = 0;
    int diceRollTotal = 0;
    for (int i = 0; i <= 100000; i++) {
      //1 die
      int count = 0;
      while (count <= 6) {
        if ((int) ((Math.random() * 6) + 1) == 1) {
          dieRoll = dieRoll + 1;
          break;
        }
        count++;
      }
      
    }
  }
}