import java.util.*; 
public class GroupRandomizer {
  public static void main(String[] args) {
     String[] apCompSci = {"Emmet", "Shingo", "Akshay", "Kevin", "Ira", "Adam", "Arman", "Malcolm", "Anthony", "Dawson", "Timmy"};
     
     // 3 groups of 3, 1 groupd 2 (with veto power)
     
     shuffleArray(apCompSci);
     for (int i = 0; i < solutionArray.length; i++) {
      System.out.println(solutionArray[i]);
    }
  
  }
}